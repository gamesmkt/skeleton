{
    "name": "fishpond-services/:package_name",
    "description": "The service for Fishpond",
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0",
        "gamesmkt/fishpond": "^1.0",
        "gamesmkt/fishpond-record": "^1.0",
        "graham-campbell/guzzle-factory": "^3",
        "guzzlehttp/guzzle": "^6.5",
        "illuminate/support": "~5.5 || ^6.8",
        "nesbot/carbon": "^2.29"
    },
    "autoload": {
        "psr-4": {
            "FishpondServices\\:service_namespace\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FishpondServices\\:service_namespace\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    }
}
